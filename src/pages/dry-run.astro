---
import EmptyLayout from '@/layouts/EmptyLayout.astro'
---

<EmptyLayout 
  title="Totem Grid - Dry Run" 
  description="Totem Grid - Dry Run">
  <style>
  body, html {
    padding: 0;
    margin: 0;
    min-height: 100vh;
    width: 100%;
  }
  :root {
    --blue: #0000ff;
    --green: #57FFD8;
    --white: #D3D3FF;
  }
  .main-wrapper {
    font-family: "Inclusive Sans", sans-serif;
    font-optical-sizing: auto;
    font-style: normal;
    position: relative;
    min-height: 100vh;
    background: var(--blue);
    padding: 40% 20% 40% 20%;
    display: flex;
    flex-direction: column;
    gap: 24px;
    color: var(--white);
  }
  p, a {
    font-family: "Inclusive Sans", sans-serif;
    font-weight: 500;
    text-align: justify;
    text-transform: uppercase;
    font-size: 1rem;
    color: var(--white);
  }
  h2, h3 {
    font-family: "Inclusive Sans", sans-serif;
    color: var(--white);
    font-weight: 900;
  }
  .wrap-text {
    position: absolute;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    height: 100%;
    width: 100%;
  }
  .fg-image {
    color: var(--green);
    pointer-events: none;
  }
  figure {
    margin: 0 auto;
    width: 100%;
    border-radius: 64px;
    overflow: hidden;
    width: 100%p
  }
  figure img {
    width: 100%;
  }
  h3 {
    margin-bottom: 20px;

  }
  .button {
    display: block;
    background: var(--green);
    padding: 20px;
    text-align: center;
    color: var(--blue);
  }
  </style>
